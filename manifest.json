{
  "name": "Issues to Inbox",
  "short_name": "issuesToInbox",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Import Github issue information to the Trello Inbox.",
  "options_page": "settings.html",
  "browser_action": {
    "default_title": "Issues to Inbox",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://*/",
    "http://*/"
  ],
  "web_accessible_resources": ["settings.html"],
  "content_security_policy": "script-src 'self' https://api.trello.com https://ajax.googleapis.com https://api.launchpad.net; object-src 'self'"
}
